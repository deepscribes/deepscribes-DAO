{
  "name": "@deepscribes/deepscribes-dao",
  "version": "20250706.0.1",
  "description": "The DAO for Deepscribes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run test:full",
    "test:full": "npm run test:unit && npm run test:integ",
    "test:unit": "jest --testMatch \"**/*.test.ts\" --testPathIgnorePatterns=\".*\\.integ\\.test\\.ts$\" --coverage",
    "test:integ": "node run_integ.js",
    "test:integration": "jest --testMatch \"**/*.integ.test.ts\"",
    "patch": "npm version \"$(date +%Y%m%d.0.1)\" --no-git-tag-version",
    "postpush": "git tag \"v$(date +%Y%m%d.0.1)\" && git push origin \"v$(date +%Y%m%d.0.1)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepscribes/deepscribes-DAO.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "DAO"
  ],
  "author": "Nicola Migone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deepscribes/deepscribes-DAO/issues"
  },
  "homepage": "https://github.com/deepscribes/deepscribes-DAO#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^30.0.2",
    "loadenv": "^2.2.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.830.0",
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/client-ses": "^3.830.0",
    "@aws-sdk/s3-request-presigner": "^3.832.0",
    "@aws-sdk/util-dynamodb": "^3.830.0",
    "@clerk/backend": "^2.1.0"
  }
}
